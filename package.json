{
  "name": "clinic-central",
  "private": true,
  "version": "1.0.0",
  "description": "Clinic Central - Doctor, Receptionist, Prescription, Medicine and Patient Management System built with React, Prisma, and Tailwind CSS",
  "homepage": "https://clinic-central.vercel.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/dhruvpatel16120/clinic-management-system.git"
  },
  "keywords": [
    "clinic",
    "management",
    "healthcare",
    "react",
    "prisma",
    "tailwindcss",
    "authentication",
    "role-based-access"
  ],
  "author": "Dhruv Patel",
  "license": "MIT",
  "type": "module",
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "scripts": {
    "dev": "vite",
    "server": "node --watch server/index.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run server\"",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "cap:sync": "npx cap sync",
    "cap:build": "npm run build && npx cap sync",
    "cap:android": "npx cap open android",
    "cap:ios": "npx cap open ios",
    "cap:run:android": "npm run build && npx cap sync && npx cap run android",
    "cap:run:ios": "npm run build && npx cap sync && npx cap run ios"
  },
  "dependencies": {
    "@capacitor/android": "^8.1.0",
    "@capacitor/app": "^8.0.1",
    "@capacitor/camera": "^8.0.1",
    "@capacitor/cli": "^8.1.0",
    "@capacitor/core": "^8.1.0",
    "@capacitor/filesystem": "^8.1.1",
    "@capacitor/haptics": "^8.0.0",
    "@capacitor/ios": "^8.1.0",
    "@capacitor/keyboard": "^8.0.0",
    "@capacitor/network": "^8.0.1",
    "@capacitor/preferences": "^8.0.1",
    "@capacitor/push-notifications": "^8.0.1",
    "@capacitor/splash-screen": "^8.0.1",
    "@capacitor/status-bar": "^8.0.1",
    "@neondatabase/serverless": "^1.0.2",
    "@prisma/adapter-neon": "^7.4.2",
    "@prisma/client": "^7.4.2",
    "@react-oauth/google": "^0.13.4",
    "@vercel/edge-config": "^1.4.3",
    "axios": "^1.13.3",
    "bcryptjs": "^3.0.3",
    "better-sqlite3": "^12.6.2",
    "concurrently": "^9.2.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.6",
    "dotenv": "^17.3.1",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.8.1",
    "lucide-react": "^0.541.0",
    "nodemailer": "^8.0.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.19.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hot-toast": "^2.6.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.8.1",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "prisma": "^7.4.2",
    "tailwindcss": "^4.1.12",
    "vite": "^7.1.2"
  }
}
